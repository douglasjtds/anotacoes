Questão 16
	public void metodo(Venda[] vet, Venda obj){
		for(int i = 0; i < vet.lenght; i++){
			if (vet[i] == null){
				vet[i] = obj;
			}
		}
	}


	public boolean metodo(Venda[] vet, Venda obj){  //melhorando a função
		for(int i = 0; i < vet.lenght; i++){
			if (vet[i] == null){
				vet[i] = obj;
				return true;
			}
		}
		return false;
	}


